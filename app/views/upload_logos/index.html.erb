<p id="notice"><%= notice %></p>

<h1>Upload Logos</h1>

<table>
  <thead>
    <tr>
      <th>Logo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_logos.each do |upload_logo| %>
      <tr>
        <td><%= image_tag upload_logo.logo.url %></td>
        <td><%= link_to 'Show', upload_logo %></td>
        <td><%= link_to 'Edit', edit_upload_logo_path(upload_logo) %></td>
        <td><%= link_to 'Destroy', upload_logo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Logo', new_upload_logo_path %>
